<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Calculator</title>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
<link rel="stylesheet" href="styles.css" type="text/css">

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
<script src='script.js' type="text/javascript"></script

<% include ../partials/navbar %>
<h1 class="heading">Scientific Calculator</h1>

<table border="2">
       <tr>
          <td colspan="3" class="text-td"><input type="text" disabled="disabled" id="result"/></td>
          <td><button class="calc-button" type="button" data-clear>C</button></td>
          <td><button class="calc-button" type="button" data-operation>&#177;</button></td>
       </tr>
       <tr>
         <td><button class="calc-button" type="button" data-operation>√</button></td>
          <td><button class="calc-button" type="button" data-number>1</button></td>
          <td><button class="calc-button" type="button" data-number>2</button></td>
          <td><button class="calc-button" type="button" data-number>3</button></td>
          <td><button class="calc-button" type="button" data-operation>/</button></td>
       </tr>
       <tr>
         <td><button class="calc-button" type="button" data-number>π</button></td>
          <td><button class="calc-button" type="button" data-number>4</button></td>
          <td><button class="calc-button" type="button" data-number>5</button></td>
          <td><button class="calc-button" type="button" data-number>6</button></td>
          <td><button class="calc-button" type="button" data-operation>-</button></td>
       </tr>
       <tr>
         <td><button class="calc-button" type="button" style="font-style: italic;" data-number>e</button></td>
          <td><button class="calc-button" type="button" data-number>7</button></td>
          <td><button class="calc-button" type="button" data-number>8</button></td>
          <td><button class="calc-button" type="button" data-number>9</button></td>
          <td><button class="calc-button" type="button" data-operation>+</button></td>
       </tr>
       <tr>
         <td><button class="calc-button" type="button" data-operation>x<sup>2</sup></button></td>
         <td><button class="calc-button" type="button" data-number>.</button></td>
         <td><button class="calc-button" type="button" data-number>0</button></td>
         <td><button class="calc-button" type="button" data-solve>=</button></td>
         <td><button class="calc-button" type="button" data-operation>X</button></td>
       </tr>
    </table>
